<div id="form" class="form-modal">
  <%= form_with(model: Friend.new, local: true) do |f| %>
    <div class="form_content-modal">
      <header></header>
      <%= f.label :name, class: "form-label" %>
      <%= f.text_field :name, class: "form-control"%>
      <%= f.label :home_number, class: "form-label" %>
      <%= f.text_field :home_number, class: "form-control"%>
      <%= f.label :mobile_number, class: "form-label" %>
      <%= f.text_field :mobile_number, class: "form-control"%>
      <%= f.label :address, class: "form-label" %>
      <%= f.text_field :address, class: "form-control"%>
      <%= f.label :email, class: "form-label" %>
      <%= f.email_field :email, class: "form-control"%>
      <%=f.submit(class: "btn btn-primary")%>
    </div>
  <%end%>
</div>
