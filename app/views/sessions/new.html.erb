<div class="form-content">
  <%= form_with(scope: :session, url: login_path, local: true) do |f| %>
  <h1>Log In</h1>
  <p>
    <%= f.label :email%>
    <%= f.email_field :email%>
  </p>
  <p>
    <%= f.label :password%>
    <%= f.password_field :password%>
  </p>
    <%=f.submit "Log In"%>
  <%end%>
</div>